---
---
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/sitemap.xsl"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">    
	<url>
		<loc>http://willowfrank.com/</loc>
		<lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>
	{% for post in site.posts %}
	<url>
		<loc>http://willowfrank.com{{ post.url }}</loc>
		<lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>0.8</priority>
	</url>
	{% endfor %}
	{% for tag in site.tags %}{% assign is_blog_tag = false %}{% for post in tag[1] %}{% if post.type != "work" %}{% assign is_blog_tag = true %}{% endif %}{% endfor %}{% if is_blog_tag == true %}
	<url>
		<loc>http://willowfrank.com/tags/{{ tag[0] }}</loc>
		<lastmod>2013-02-10T20:59:36+00:00</lastmod>
		<changefreq>weekly</changefreq>
		<priority>0.6</priority>
	</url>
	{% endif %}{% endfor %}
</urlset>